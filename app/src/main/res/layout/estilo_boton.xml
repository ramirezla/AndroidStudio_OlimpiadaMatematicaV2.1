<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle"
    android:layout_height="6dp"
    android:layout_width="6dp">
    <corners
        android:radius="14dp"
        />
    <gradient
        android:angle="90"
        android:centerX="15%"
        android:centerColor="#D1CDD0"
        android:startColor="#E8E8E8"
        android:endColor="#808080"
        android:type="linear"
        />
    <padding
        android:left="0dp"
        android:top="0dp"
        android:right="0dp"
        android:bottom="0dp"
        />
    <size
        android:width="180dp"
        android:height="60dp"
        />
    <stroke
        android:width="3dp"
        android:color="#878787"
        />
</shape>